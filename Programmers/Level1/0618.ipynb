    "def solution(s):\n",
    "    words = [\"zero\", \"one\", \"two\", \"three\", \"four\", \"five\", \"six\", \"seven\", \"eight\", \"nine\"]\n",
    "    for i, word in enumerate(words):\n",
    "        s = s.replace(word, str(i))\n",
    "\n",
    "    return int(s)"
