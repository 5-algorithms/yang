    "def solution(a, b):\n",
    "    week = [\"FRI\",\"SAT\",\"SUN\",\"MON\",\"TUE\",\"WED\",\"THU\"]\n",
    "    days = [0, 31, 29, 31, 30, 31, 30, 31, 31, 30, 31, 30]\n",
    "    \n",
    "    count_day = sum(days[:a]) + b - 1\n",
    "    return week[count_day % 7]"
   ]
